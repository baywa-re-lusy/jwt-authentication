{
    "name": "baywa-re-lusy/jwt-authentication",
    "description": "BayWa r.e. LUSY JWT Authentication",
    "type": "library",
    "license": "proprietary",
    "homepage": "https://github.com/baywa-re-lusy/jwt-authentication",
    "require": {
        "php": ">=8.1",
        "ext-mbstring": "*",
        "laminas/laminas-hydrator": "^4.3",
        "firebase/php-jwt": "^6.3",
        "guzzlehttp/guzzle": "^7.4",
        "guzzlehttp/psr7": "^2.4",
        "psr/cache": "^3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "BayWaReLusy\\JwtAuthentication\\": "src/JwtAuthentication"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BayWaReLusy\\JwtAuthentication\\Test\\": "tests"
